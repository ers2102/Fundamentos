
#include <iostream>
#include <conio.h>

using namespace std;

double promedio (double a[],int tamanio);

int main()
{
    double x[]={3.5,6.3,4.5,7.5,5.4};
    cout << promedio (x,5)<< endl;
    getch();

    return 0;
}

double promedio (double a[],int tamanio)
{
    double suma=0;
    for (int i=0; i<tamanio;i++)
    suma += a[i];
    return suma/tamanio;
}
