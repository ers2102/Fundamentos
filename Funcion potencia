
#include <iostream>

using namespace std;

int poten(int x, int y);

int main()
{
    int x, y;
    cout<<"Ingresar valor base: "<<endl;
    cin>>x;
    cout<<"Ingresar exponente: "<<endl;
    cin>>y;
    cout<<"El resultado es: "<<poten(x,y);

    return 0;
}

int poten(int x, int y)
{
    int acum=1;
    for (int i=1; i<=y; i++)
    acum=acum*x;   
    return acum;
}
